{
  "name": "puppet-sftp_jail",
  "version": "2.0.2-rc0",
  "author": "Vox Pupuli",
  "summary": "Leverage saz-ssh module to build SFTP jails",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-sftp_jail.git",
  "project_page": "https://github.com/voxpupuli/puppet-sftp_jail",
  "issues_url": "https://github.com/voxpupuli/puppet-sftp_jail.git",
  "dependencies": [
    {
      "name": "saz/ssh",
      "version_requirement": ">= 2.9.1 < 3.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.10.0 < 6.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ]
}
