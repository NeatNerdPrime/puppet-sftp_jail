{
  "name": "puppet-sftp_jail",
  "version": "5.0.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Leverage saz-ssh module to build SFTP jails",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-sftp_jail.git",
  "project_page": "https://github.com/voxpupuli/puppet-sftp_jail",
